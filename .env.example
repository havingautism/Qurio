# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_KEY=your-anon-key-here

# OpenAI Compatible API (Optional)
NEXT_PUBLIC_OPENAI_API_KEY=your-openai-api-key
NEXT_PUBLIC_OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_API_KEY=your-openai-api-key
OPENAI_BASE_URL=https://api.openai.com/v1

# SiliconFlow API (Optional)
NEXT_PUBLIC_SILICONFLOW_API_KEY=your-siliconflow-api-key
# Base URL is fixed to https://api.siliconflow.cn/v1/

NEXT_PUBLIC_GOOGLE_API_KEY=your-google-api-key
GOOGLE_API_KEY=your-google-api-key
